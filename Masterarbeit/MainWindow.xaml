<Window x:Class="Masterarbeit.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Masterarbeit"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="500" >
    <Grid>
        <Button x:Name="start_button" Content="Start Tracker" HorizontalAlignment="Left" Margin="196,126,0,0" VerticalAlignment="Top" Width="90" Click="start_button_Click"/>
        <Button x:Name="stop_button" Content="Stop Tracker" HorizontalAlignment="Left" Margin="196,169,0,0" VerticalAlignment="Top" Width="90" IsEnabled="False" Click="stop_button_Click"/>
        <CheckBox x:Name="checkBox" Content="Remove Errors" HorizontalAlignment="Left" Margin="322,129,0,0" VerticalAlignment="Top" Width="123" Checked="checkBox_Checked" Unchecked="checkBox_Unchecked"/>
        <Label x:Name="label" x:FieldModifier="public" Content="Label" HorizontalAlignment="Left" Margin="10,266,0,0" VerticalAlignment="Top" Width="472" Height="45" />
        <CheckBox x:Name="chkVM" Content="Use VM" HorizontalAlignment="Left" Margin="322,174,0,0" VerticalAlignment="Top" Width="123" Checked="chkVM_Checked" />

    </Grid>



</Window>
